<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.luxixi.mapper.StudentInfoDao">
    <sql id="Table_Name">
        student_info
    </sql>

    <sql id="BaseColumnList">
    `id` AS id,
    `stu_name` AS stu_name,
    `stu_gender` AS stu_gender,
    `stu_age` AS stu_age,
    `stu_birthday` AS stu_birthday
    </sql>

    <resultMap id="BaseResultMap" type="com.luxixi.domain.entity.StudentInfoEntity">
        <result column="id" property="id" jdbcType="INTEGER"></result>
        <result column="stu_name" property="id" jdbcType="VARCHAR"></result>
        <result column="stu_gender" property="id" jdbcType="INTEGER"></result>
        <result column="stu_age" property="id" jdbcType="INTEGER"></result>
        <result column="stu_birthday" property="id" jdbcType="TIMESTAMP"></result>
    </resultMap>

</mapper>
